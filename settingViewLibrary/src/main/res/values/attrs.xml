<resources>

    <!-- SettingView -->
    <declare-styleable name="SettingView">
        <attr name="iOSStyle" format="boolean" />
    </declare-styleable>
    <declare-styleable name="SettingViewItem">
        <attr name="setting_view_item_title" format="string" />
        <attr name="setting_view_item_subTitle" format="string" />
        <attr name="setting_view_item_titleColor" format="reference" />
        <attr name="setting_view_item_subTitleColor" format="reference" />
        <attr name="setting_view_item_titleSize" format="reference|dimension" />
        <attr name="setting_view_item_subTitleSize" format="reference|dimension" />
        <attr name="setting_view_item_drawable" format="reference" />
        <attr name="setting_view_item_arrow" format="reference" />
        <attr name="setting_view_item_check" format="reference" />
        <attr name="setting_view_item_image" format="reference" />
        <attr name="setting_view_item_clickable" format="boolean" />
        <attr name="setting_view_item_checked" format="boolean" />
        <attr name="setting_view_item_background" format="reference" />
    </declare-styleable>
    <!-- SettingView -->

</resources>